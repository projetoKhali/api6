{
  "name": "api",
  "targets": {
    "install": {
      "executor": "nx:run-commands",
      "options": {
        "command": "poetry install"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "dependsOn": ["install"],
      "options": {
        "command": "poetry run python {projectRoot}/main.py"
      }
    },
    "add": {
      "executor": "nx:run-commands",
      "options": {
        "command": "poetry add {args.dep}"
      }
    },
    "lock": {
      "executor": "nx:run-commands",
      "options": {
        "command": "poetry lock",
        "cwd": "{projectRoot}"
      }
    }
  }
}
